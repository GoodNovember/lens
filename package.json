{
  "name": "lens",
  "version": "1.0.0",
  "main": "src/electron.js",
  "license": "MIT",
  "scripts": {
    "ignite": "node ignite.js",
    "start": "node ignite.js",
    "boot-electron": "electron .",
    "react-start": "parcel -p 3000 index.html --out-dir build",
    "react-build": "parcel build index.html --out-dir build --public-url ./",
    "electron-build": "electron-builder -mwl",
    "clean-build": "rm -rf build/ .cache dist/",
    "build": "yarn clean-build && yarn react-build && yarn electron-build",
    "fairy": "parcel ./fairy/index.html --out-dir ./fairy/build"
  },
  "dependencies": {
    "acorn": "^7.0.0",
    "acorn-walk": "^7.0.0",
    "astring": "^1.4.1",
    "babylonjs": "^4.0.3",
    "babylonjs-gui": "^4.0.3",
    "electron-fs-extra": "^1.0.0",
    "electron-is-dev": "^1.1.0",
    "esprima": "^4.0.1",
    "estree-walker": "^0.8.1",
    "eventemitter3": "^4.0.0",
    "express": "^4.17.1",
    "javascript-state-machine": "^3.1.0",
    "micromatch": "^4.0.2",
    "mime": "^2.4.4",
    "natural-order": "^0.2.3",
    "pixi-layers": "^0.2.1",
    "pixi.js": "^5.1.0",
    "pixi.js-legacy": "^5.1.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sane": "^4.1.0",
    "standard": "^14.2.0",
    "styled-components": "^4.3.2",
    "three": "^0.107.0",
    "tildify": "^2.0.0",
    "untildify": "^4.0.0",
    "verlet": "^1.0.0"
